                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Empleado">
                                    <f:fields>
                                        <Select id="selectEmpleado" selectedKey="1">
                                            <items>
                                                <core:Item text="empleado 1" key="1" />
                                                <core:Item text="empleadp 2" key="2" />
                                                <core:Item text="empleado 3" key="3" />
                                            </items>
                                        </Select>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement label="Equipo">
                                    <f:fields>
                                        <Select id="selectEquipo" selectedKey="1">
                                            <items>
                                                <core:Item text="equipo 1" key="1" />
                                                <core:Item text="equipo 2" key="2" />
                                                <core:Item text="equipo 3" key="3" />
                                            </items>
                                        </Select>
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
